.midi
    position absolute
    top 0
    left 10px
    padding 10px
    background: rgba(0,0,0,0.8)
    color #fff
    z-index 1000
    font-size 11px
    line-height: 14px

.gui-pad

    position absolute
    width 200px
    height 200px
    padding 10px
    background rgba(0,0,0,0.8)
    bottom 20px
    right 20px
    z-index 1000
    border 1px solid #666

    div
        position relative
        width 14px
        height 14px
        margin 3px 4px
        background #00F
        padding 0
        border 1px solid #0F0
        display inline-block
        top 0
        right 0
